; tests writing of basic smtlib-load with string rather than file input

(def str "(set-info :smt-lib-version 2.0)(set-logic QF_NRA)(declare-fun x () Real)(declare-fun y () Real)(declare-fun z () Real)(assert (< (+ z 1) (/ 1 (- x (/ 1 y)))))(check-sat)")
(def out (smtlib-load 'clear str))
(def chk [(x y - 1)(x y z - z + x y - y - 1) < 0])
(def res (if (equal? out chk) "pass" "fail"))
(display res " : test smt11\n")

