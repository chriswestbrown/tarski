      (if (equal? (evalf (head (head D)) (head (tail (head D))) F) [true]) "sat" "sat-error!"))
)
(display (if (equal? res target) "pass" "fail") " : test sat02 - b\n")

(def F [ 16 x+2 y z-35 z-17 > 0 /\ -97 x-46 y+45 z x+75 > 0 /\ -45 x+11 y+z x-8 > 0 /\ -79 x+97 y z-50 z-69 > 0 /\ 30 x-43 y^2-51 z-23 > 0 /\ -74 x-81 y^2-12 z+23 > 0 ] )
(def V '(x y z))
